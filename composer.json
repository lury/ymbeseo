{
	"name": "senlin/ymbeseo",
	"type": "wordpress-plugin",
	"description": "YMBE SEO - fork of Yoast SEO 2.3.5 back to basics",
	"homepage": "https://so-wp.com/plugins/ymbeseo",
	"license": "GPL3+",
	"authors": [
		{
			"name": "Piet Bos",
			"homepage": "https://github.com/senlin"
		}
	],
	"keywords": [
		"wordpress", "yoast", "seo"
	],
	"support": {
		"issues": "https://github.com/senlin/ymbeseo/issues"
	},
	"require": {
		"composer/installers": "~1.0",
		"yoast/i18n-module": "^1.0",
		"xrstf/composer-php52": "^1.0.17"
	},
	"require-dev": {
		"yoast/yoastcs": "dev-master"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"autoload": {
		"classmap": [
			"admin/",
			"frontend/",
			"inc/"
		]
	},
	"scripts": {
		"config-yoastcs": [
			"\"vendor/bin/phpcs\" --config-set installed_paths ../../../vendor/wp-coding-standards/wpcs,../../../vendor/yoast/yoastcs",
			"\"vendor/bin/phpcs\" --config-set default_standard Yoast"
		],
		"post-install-cmd": [
			"xrstf\\Composer52\\Generator::onPostInstallCmd"
		],
		"post-update-cmd": [
			"xrstf\\Composer52\\Generator::onPostInstallCmd"
		],
		"post-autoload-dump": [
			"xrstf\\Composer52\\Generator::onPostInstallCmd"
		]
	}
}
